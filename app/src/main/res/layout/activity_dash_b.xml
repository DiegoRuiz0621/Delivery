<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#3F51B5"
    tools:context=".DashB">

    <Button
        android:id="@+id/button21"
        android:layout_width="254dp"
        android:layout_height="41dp"
        android:layout_marginTop="168dp"
        android:backgroundTint="#FFC107"
        android:gravity="center"
        android:text="Postres"
        app:layout_constraintTop_toBottomOf="@+id/button7"
        tools:layout_editor_absoluteX="72dp" />

    <Button
        android:id="@+id/button20"
        android:layout_width="254dp"
        android:layout_height="41dp"
        android:layout_marginTop="128dp"
        android:backgroundTint="#FFC107"
        android:gravity="center"
        android:text="Saludables"
        app:layout_constraintTop_toBottomOf="@+id/button7"
        tools:layout_editor_absoluteX="71dp" />

    <Button
        android:id="@+id/button19"
        android:layout_width="254dp"
        android:layout_height="41dp"
        android:layout_marginTop="88dp"
        android:backgroundTint="#FFC107"
        android:gravity="center"
        android:text="Mexicana"
        app:layout_constraintTop_toBottomOf="@+id/button7"
        tools:layout_editor_absoluteX="71dp" />

    <Button
        android:id="@+id/button18"
        android:layout_width="254dp"
        android:layout_height="41dp"
        android:layout_marginTop="48dp"
        android:backgroundTint="#FFC107"
        android:gravity="center"
        android:text="Sushi"
        app:layout_constraintTop_toBottomOf="@+id/button7"
        tools:layout_editor_absoluteX="71dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Res. Isla Arcoiris"
        android:textColor="#F6E6E6"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="44dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ubicacion"
        android:textColor="#F6E6E6"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="246dp"
        android:layout_height="225dp"
        android:src="@drawable/ic_launcher_background"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.436"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="104dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="OFERTAS DE RESTAURANTES"
        app:layout_constraintBottom_toBottomOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="@+id/imageView5"
        tools:layout_editor_absoluteX="112dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="253dp"
        android:layout_height="45dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="#FFC107"
        android:gravity="center"
        android:text="PIZZA"
        app:layout_constraintBottom_toTopOf="@+id/button7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/button7"
        android:layout_width="254dp"
        android:layout_height="40dp"
        android:backgroundTint="#FFC107"
        android:gravity="center"
        android:text="TACOS"
        app:strokeColor="#FFC107"
        tools:layout_editor_absoluteX="72dp"
        tools:layout_editor_absoluteY="399dp" />

    <Button
        android:id="@+id/button8"
        android:layout_width="254dp"
        android:layout_height="41dp"
        android:layout_marginTop="4dp"
        android:backgroundTint="#FFC107"
        android:gravity="center"
        android:text="Hamburguesas"
        app:layout_constraintTop_toBottomOf="@+id/button7"
        tools:layout_editor_absoluteX="71dp" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="70dp"
        android:layout_height="72dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="61dp"
        android:layout_height="87dp"
        android:src="@drawable/engrane2"
        tools:layout_editor_absoluteX="334dp"
        tools:layout_editor_absoluteY="7dp" />

</androidx.constraintlayout.widget.ConstraintLayout>